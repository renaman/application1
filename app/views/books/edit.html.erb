<h2>Editing Book</h2>

<%= form_for(@book, url:"/books/#{@book.id}") do |f| %>
	<h4>Title</h4>
	<%= f.text_field :title %>
	<h4>Body</h4>
	<%= f.text_area :body %>
	<%= f.submit 'Update Book' %>

<% end %>

	<%=link_to "show", show_books_path(@book.id) %>
	<%=link_to "Back", books_path(@book.id) %>
